    "  return naive_bayes\n",
    "\n",
    "var = None\n",
    "naive_bayes = None\n",
    "try:\n",
    "  naive_bayes = naive_bayes()\n",
    "except:\n",
    "  var = traceback.format_exc()\n",
    "\n",
    "if var == None:\n",
    "  telegram_bot_sendtext(naive_bayes)\n",
    "else:\n",
