    "test_data = pd.read_csv(\"../data/test.csv\",index_col=False)\n",
    "test_data_labels = pd.read_csv(\"../data/test_labels.csv\",index_col=False)\n",
    "test_data_labels.head()\n",
    "print(type(test_data))"
   ]
